<?xml version="1.0" encoding="utf-8"?>
<com.tateam.languageofsurvival.app.FractionFrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:card_view="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <ScrollView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:scrollbarSize="@dimen/margin_0_1"
            android:scrollbarThumbVertical="@drawable/scrollbar">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:gravity="center"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center_horizontal"
                    android:orientation="horizontal"
                    >


                    <android.support.v7.widget.CardView xmlns:card_view="http://schemas.android.com/apk/res-auto"
                        android:id="@+id/buttonMeeting"
                        android:layout_width="@dimen/home_screen_button_width"
                        android:layout_height="@dimen/home_screen_button_height"
                        android:layout_gravity="center"
                        android:layout_marginLeft="@dimen/margin_5"
                        android:gravity="center"
                        card_view:cardCornerRadius="@dimen/card_view_corner_radius"
                        card_view:cardElevation="@dimen/card_view_elevation"
                        card_view:cardUseCompatPadding="true">

                        <tatteam.com.app_common.ui.drawable.FlatEffectDrawable
                            android:id="@+id/flatMeeting"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_gravity="center">

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:layout_gravity="center"
                                android:gravity="center"
                                android:orientation="vertical">

                                <ImageView
                                    android:layout_width="match_parent"
                                    android:layout_height="@dimen/height_row"
                                    android:layout_gravity="center"
                                    android:src="@drawable/chat_version2_trans"

                                    />

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_gravity="center"
                                    android:maxLines="1"
                                    android:text="Meeting"
                                    android:textColor="@color/colorPrimaryDark"
                                    android:textSize="@dimen/text_size" />
                            </LinearLayout>
                        </tatteam.com.app_common.ui.drawable.FlatEffectDrawable>

                    </android.support.v7.widget.CardView>


                    <android.support.v7.widget.CardView xmlns:card_view="http://schemas.android.com/apk/res-auto"
                        android:id="@+id/buttonRestaurant"
                        android:layout_width="@dimen/home_screen_button_width"
                        android:layout_height="@dimen/home_screen_button_height"
                        android:layout_marginRight="@dimen/margin_5"
                        card_view:cardCornerRadius="@dimen/card_view_corner_radius"
                        card_view:cardElevation="@dimen/card_view_elevation"
                        card_view:cardUseCompatPadding="true">

                        <tatteam.com.app_common.ui.drawable.FlatEffectDrawable
                            android:id="@+id/flatRestaurant"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_gravity="center"
                            android:gravity="center">

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:layout_gravity="center"
                                android:gravity="center"
                                android:orientation="vertical">

                                <ImageView
                                    android:layout_width="match_parent"
                                    android:layout_height="@dimen/height_row"
                                    android:layout_gravity="center"
                                    android:src="@drawable/restaurant_blue_icon_mini" />

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_gravity="center"
                                    android:maxLines="1"
                                    android:text="@string/restaurant"
                                    android:textColor="@color/colorPrimaryDark"
                                    android:textSize="@dimen/text_size" />
                            </LinearLayout>
                        </tatteam.com.app_common.ui.drawable.FlatEffectDrawable>

                    </android.support.v7.widget.CardView>


                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/margin_top_row"
                    android:gravity="center_horizontal"
                    android:orientation="horizontal">


                    <android.support.v7.widget.CardView xmlns:card_view="http://schemas.android.com/apk/res-auto"
                        android:id="@+id/buttonDaily"
                        android:layout_width="@dimen/home_screen_button_width"
                        android:layout_height="@dimen/home_screen_button_height"
                        android:layout_below="@+id/buttonMeeting"
                        android:layout_marginLeft="@dimen/margin_5"
                        card_view:cardCornerRadius="@dimen/card_view_corner_radius"
                        card_view:cardElevation="@dimen/card_view_elevation"
                        card_view:cardUseCompatPadding="true">

                        <tatteam.com.app_common.ui.drawable.FlatEffectDrawable
                            android:id="@+id/flatDaily"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_gravity="center"
                            android:gravity="center">

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:layout_gravity="center"
                                android:gravity="center"
                                android:orientation="vertical">

                                <ImageView
                                    android:layout_width="match_parent"
                                    android:layout_height="@dimen/height_row"
                                    android:layout_gravity="center"
                                    android:src="@drawable/calendar_icon_orange_trans_ver2" />

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_gravity="center"
                                    android:maxLines="1"
                                    android:text="@string/daily"
                                    android:textColor="@color/colorPrimaryDark"
                                    android:textSize="@dimen/text_size" />
                            </LinearLayout>
                        </tatteam.com.app_common.ui.drawable.FlatEffectDrawable>
                    </android.support.v7.widget.CardView>


                    <android.support.v7.widget.CardView xmlns:card_view="http://schemas.android.com/apk/res-auto"
                        android:id="@+id/buttonSocializing"
                        android:layout_width="@dimen/home_screen_button_width"
                        android:layout_height="@dimen/home_screen_button_height"
                        android:layout_below="@+id/buttonMeeting"
                        android:layout_marginRight="@dimen/margin_5"
                        card_view:cardCornerRadius="@dimen/card_view_corner_radius"
                        card_view:cardElevation="@dimen/card_view_elevation"
                        card_view:cardUseCompatPadding="true">

                        <tatteam.com.app_common.ui.drawable.FlatEffectDrawable
                            android:id="@+id/flatSocializing"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_gravity="center"
                            android:gravity="center">

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:layout_gravity="center"
                                android:gravity="center"
                                android:orientation="vertical">

                                <ImageView
                                    android:layout_width="match_parent"
                                    android:layout_height="@dimen/height_row"
                                    android:layout_gravity="center"
                                    android:src="@drawable/socializing_icon_version3_trans" />

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_gravity="center"
                                    android:maxLines="1"
                                    android:text="@string/socializing"
                                    android:textColor="@color/colorPrimaryDark"
                                    android:textSize="@dimen/text_size" />
                            </LinearLayout>
                        </tatteam.com.app_common.ui.drawable.FlatEffectDrawable>


                    </android.support.v7.widget.CardView>


                </LinearLayout>


                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/margin_top_row"
                    android:gravity="center_horizontal"
                    android:orientation="horizontal">


                    <android.support.v7.widget.CardView xmlns:card_view="http://schemas.android.com/apk/res-auto"
                        android:id="@+id/buttonShopping"
                        android:layout_width="@dimen/home_screen_button_width"
                        android:layout_height="@dimen/home_screen_button_height"
                        android:layout_below="@+id/buttonDaily"
                        android:layout_marginLeft="@dimen/margin_5"
                        card_view:cardCornerRadius="@dimen/card_view_corner_radius"
                        card_view:cardElevation="@dimen/card_view_elevation"
                        card_view:cardUseCompatPadding="true">

                        <tatteam.com.app_common.ui.drawable.FlatEffectDrawable
                            android:id="@+id/flatShopping"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_gravity="center"
                            android:gravity="center">

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:layout_gravity="center"
                                android:gravity="center"
                                android:orientation="vertical">

                                <ImageView
                                    android:layout_width="match_parent"
                                    android:layout_height="@dimen/height_row"
                                    android:layout_gravity="center"
                                    android:src="@drawable/fix_shopping_transparent_ver2" />

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_gravity="center"
                                    android:maxLines="1"
                                    android:text="@string/shopping"
                                    android:textColor="@color/colorPrimaryDark"
                                    android:textSize="@dimen/text_size" />

                            </LinearLayout>
                        </tatteam.com.app_common.ui.drawable.FlatEffectDrawable>

                    </android.support.v7.widget.CardView>


                    <android.support.v7.widget.CardView xmlns:card_view="http://schemas.android.com/apk/res-auto"
                        android:id="@+id/buttonHotel"
                        android:layout_width="@dimen/home_screen_button_width"
                        android:layout_height="@dimen/home_screen_button_height"
                        android:layout_below="@+id/buttonDaily"
                        android:layout_marginRight="@dimen/margin_5"
                        card_view:cardCornerRadius="@dimen/card_view_corner_radius"
                        card_view:cardElevation="@dimen/card_view_elevation"
                        card_view:cardUseCompatPadding="true">

                        <tatteam.com.app_common.ui.drawable.FlatEffectDrawable
                            android:id="@+id/flatHotel"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_gravity="center">

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:layout_gravity="center"
                                android:gravity="center"
                                android:orientation="vertical">

                                <ImageView
                                    android:layout_width="match_parent"
                                    android:layout_height="@dimen/height_row"
                                    android:layout_gravity="center"
                                    android:src="@drawable/hotel_icon_3d_trans" />

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_gravity="center"
                                    android:maxLines="1"
                                    android:text="@string/hotel"
                                    android:textColor="@color/colorPrimaryDark"
                                    android:textSize="@dimen/text_size" />
                            </LinearLayout>
                        </tatteam.com.app_common.ui.drawable.FlatEffectDrawable>


                    </android.support.v7.widget.CardView>

                </LinearLayout>


                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/margin_top_row"
                    android:gravity="center_horizontal"
                    android:orientation="horizontal">


                    <android.support.v7.widget.CardView xmlns:card_view="http://schemas.android.com/apk/res-auto"
                        android:id="@+id/buttonDirection"
                        android:layout_width="@dimen/home_screen_button_width"
                        android:layout_height="@dimen/home_screen_button_height"
                        android:layout_below="@+id/buttonShopping"
                        android:layout_marginLeft="@dimen/margin_5"
                        card_view:cardCornerRadius="@dimen/card_view_corner_radius"
                        card_view:cardElevation="@dimen/card_view_elevation"
                        card_view:cardUseCompatPadding="true">

                        <tatteam.com.app_common.ui.drawable.FlatEffectDrawable
                            android:id="@+id/flatDirection"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_gravity="center">

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:layout_gravity="center"
                                android:gravity="center"
                                android:orientation="vertical">

                                <ImageView
                                    android:layout_width="match_parent"
                                    android:layout_height="@dimen/height_row"
                                    android:layout_gravity="center"
                                    android:src="@drawable/direction_icon_version2" />

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_gravity="center"
                                    android:maxLines="1"
                                    android:text="@string/direction"
                                    android:textColor="@color/colorPrimaryDark"
                                    android:textSize="@dimen/text_size" />
                            </LinearLayout>

                        </tatteam.com.app_common.ui.drawable.FlatEffectDrawable>

                    </android.support.v7.widget.CardView>


                    <android.support.v7.widget.CardView xmlns:card_view="http://schemas.android.com/apk/res-auto"
                        android:id="@+id/buttonWeather"
                        android:layout_width="@dimen/home_screen_button_width"
                        android:layout_height="@dimen/home_screen_button_height"
                        android:layout_below="@+id/buttonShopping"
                        android:layout_marginRight="@dimen/margin_5"
                        card_view:cardCornerRadius="@dimen/card_view_corner_radius"
                        card_view:cardElevation="@dimen/card_view_elevation"
                        card_view:cardUseCompatPadding="true">

                        <tatteam.com.app_common.ui.drawable.FlatEffectDrawable
                            android:id="@+id/flatWeather"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_gravity="center">

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:layout_gravity="center"
                                android:gravity="center"
                                android:orientation="vertical">

                                <ImageView
                                    android:layout_width="match_parent"
                                    android:layout_height="@dimen/height_row"
                                    android:layout_gravity="center"
                                    android:src="@drawable/weather_icon_version4_trans" />

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_gravity="center"
                                    android:maxLines="1"
                                    android:text="@string/weather"
                                    android:textColor="@color/colorPrimaryDark"
                                    android:textSize="@dimen/text_size" />
                            </LinearLayout>
                        </tatteam.com.app_common.ui.drawable.FlatEffectDrawable>

                    </android.support.v7.widget.CardView>


                </LinearLayout>

            </LinearLayout>
        </ScrollView>

    </LinearLayout>


</com.tateam.languageofsurvival.app.FractionFrameLayout>
